             case 'binary':
            case 'bin':
            case '!biner':
                exports.method = {
                    encode: input => {
                        return input.toString().split('').map(c => c.charCodeAt(0).toString(2))
                    }
                }
                const input = args.join(' ')
                if (!input) {
                    return client.reply(from, 'âš ï¸ Harap masukan teks! [WRONG FORMAT]', id)
                } else {
                    client.sendText(from, this.method.encode(input).join(' '))
                }
            break
